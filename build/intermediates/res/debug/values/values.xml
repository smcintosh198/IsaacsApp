    <!-- From: file:/C:/Users/Sid/GitWorkspace/IsaacsApp/res/values/styles.xml -->
    <eat-comment/>
    <style name="button">
    <item name="android:layout_width">fill_parent</item>
    <item name="android:layout_height">wrap_content</item>
  </style>
    <style name="clickablerow">
    <item name="android:layout_width">fill_parent</item>
 		<item name="android:layout_height">wrap_content</item>
 		<item name="android:weightSum">1.0</item>
 		<item name="android:focusable">true</item>
 		<item name="android:clickable">true</item>
 		<item name="android:padding">4dp</item>
		<item name="android:orientation">horizontal</item>
		<item name="android:background">@color/title_color</item>		
	</style>
    <style name="field">
    <item name="android:layout_width">0dp</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:layout_weight">2</item>
    <item name="android:textStyle">normal</item>
    <item name="android:textSize">14sp</item>
    <item name="android:textColor">#333</item>
    <item name="android:inputType">text</item>
  </style>
    <style name="field.textArea">
    <item name="android:layout_width">fill_parent</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:layout_weight">1</item>
    <item name="android:inputType">textMultiLine</item>
    <item name="android:maxLength">2000</item>
    <item name="android:gravity">top</item>
  </style>
    <style name="instructions">
    <item name="android:layout_width">wrap_content</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:textStyle">bold</item>
    <item name="android:textSize">14sp</item>
    <item name="android:textColor">#3300CC</item>
    <item name="android:paddingRight">2dp</item>
  </style>
    <style name="label">
    <item name="android:layout_width">0dp</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:layout_weight">1</item>
    <item name="android:textStyle">bold</item>
    <item name="android:textSize">14sp</item>
    <item name="android:textColor">#333</item>
  </style>
    <style name="label.noWeight">
    <item name="android:layout_weight">0</item>
    <item name="android:layout_width">fill_parent</item>
    <item name="android:layout_marginTop">6dp</item>
    <item name="android:layout_marginBottom">6dp</item>
    <item name="android:paddingLeft">4dp</item>
  </style>
    <style name="mainContainer">
		<item name="android:background">#CCC</item>
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">fill_parent</item>
		<item name="android:fillViewport">true</item>
	</style>
    <style name="row">
		<item name="android:orientation">horizontal</item>
    <item name="android:layout_width">fill_parent</item>
 		<item name="android:layout_height">wrap_content</item>
 		<item name="android:padding">2dp</item>
	</style>
    <style name="row.first">
    <item name="android:padding">4dp</item>
    <item name="android:layout_marginBottom">8dp</item>
  </style>
    <style name="rowDivider">
		<item name="android:layout_height">1dp</item>
		<item name="android:layout_width">fill_parent</item>
    <item name="android:background">#DDD</item>
	</style>
    <style name="text">
		<item name="android:textSize">16dp</item>
		<item name="android:textColor">#FFF</item>
	</style>
    <style name="text.menu">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_gravity">center_vertical</item>
		<item name="android:layout_weight">1</item>
		<item name="android:textStyle">bold</item>
		<item name="android:shadowColor">#80000000</item>
		<item name="android:shadowDx">1.0</item>
		<item name="android:shadowDy">1.0</item>
		<item name="android:shadowRadius">1.0</item>
	</style>
